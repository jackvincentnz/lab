meta {
  name: Start Chat
  type: graphql
  seq: 1
}

post {
  url: http://localhost:8080/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  mutation {
    startChat(input:{userPrompt:"What can you do?"}) {
      code
      success
      message
      chat {
        id
        messages {
          id
          type
          status
          content
          createdAt
          updatedAt
        }
        createdAt
        updatedAt
      }
    }
  }
}

settings {
  encodeUrl: true
}
