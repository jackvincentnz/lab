meta {
  name: Chat
  type: graphql
  seq: 3
}

post {
  url: http://localhost:8080/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    chat(id:"{{chatId}}"){
      id
      messages {
        id
        type
        status
        content
        toolCalls {
          id
          name
          arguments
          status
        }
      }
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
