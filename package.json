{
  "name": "lab",
  "private": true,
  "engines": {
    "pnpm": ">=8.11.0",
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "bazel build //...",
    "test": "bazel test //...",
    "lint": "pre-commit run eslint --all-files",
    "format": "pre-commit run prettier --all-files",
    "start": "bazel run //:start",
    "e2e": " cypress run",
    "start-then-e2e": "bazel run //:start && wait-on http-get://localhost:3001/actuator/health && cypress run"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@emotion/react": "^11.11.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@lab/bubbles": "workspace:*",
    "@mantine/core": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@tabler/icons-react": "^2.24.0",
    "antd": "^5.11.1",
    "graphql": "16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "screenfull": "^6.0.2",
    "web-vitals": "3.4.0"
  },
  "devDependencies": {
    "@apollo/rover": "^0.20.0",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@jest/globals": "^29.7.0",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/react-vite": "7.5.3",
    "@storybook/testing-library": "^0.2.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.10",
    "@types/node": "18.19.3",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/ui": "0.34.7",
    "cypress": "^13.3.2",
    "eslint": "8.48.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "7.33.2",
    "jest-cli": "^29.5.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "23.0.1",
    "prettier": "2.8.8",
    "react-scripts": "^5.0.1",
    "storybook": "^7.4.0",
    "tsconfig-to-swcconfig": "2.7.0",
    "typescript": "4.9.5",
    "vite": "5.0.5",
    "vitest": "0.34.6",
    "wait-on": "^7.2.0"
  },
  "pnpm": {
    "overrides": {
      "@vitejs/plugin-react": "4.2.0"
